body
  padding-top 60px
  padding-bottom 80px

.navbar-right .navbar-form
  input
    margin-right 20px
  a.btn
    margin-left 20px

.pull-right
  button
    margin-right 5px

.toast-top-right
  top 70px

h2
  margin 0
  color #666
  padding-top 90px
  font-size 52px
  font-family "trebuchet ms", sans-serif

.item
  background #333
  text-align center
  height 300px !important

.carousel
  margin-top 20px

.bs-example
  margin 20px

.icon
  width 16%
  height 16%

table thead tr th
  text-align center

.col-product
  margin-right 10px

.image-container img
  width 100%

table tbody tr td a.btn-lg
  padding 10px 6px

#loadingmsg
  color: black;
  background: #fff;
  padding: 10px;
  position: fixed;
  top: 50%;
  left: 48%;
  z-index: 100;
  border-radius: 5px;
  margin-left: -5%;
  margin-right: -25%;
  margin-bottom: -25%;

#loadingover
  background: black;
  z-index: 99;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
  filter: alpha opacity 80;
  -moz-opacity: 0.8;
  -khtml-opacity: 0.8;
  opacity: 0.8;

.pointer
  cursor pointer