.container
    .jumbotron(ng-show="isLogged")
        h2
            | Your Products
        .row
            .col-lg-3(ng-repeat="product in products")
                | {{ product.name }} {{ product.category }}
    .jumbotron
        h2
            | Catalogue Products
        .row
            .col-lg-3(ng-repeat="catalogueProduct in catalogueProducts")
                | {{ catalogueProduct.name }} {{ catalogueProduct.category }}